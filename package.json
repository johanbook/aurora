{
  "name": "@johanbook/aurora",
  "version": "0.0.1",
  "author": "Johan Book",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johanbook/aurora.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johanbook/aurora/issues"
  },
  "homepage": "https://github.com/johanbook/aurora#readme",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && cp -r package.json dist/ && npm run transpile",
    "build:docs": "styleguidist build",
    "build:types": "tsc -p ./tsconfig.types.json --emitDeclarationOnly",
    "format": "eslint --fix 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "release": "standard-version",
    "serve:docs": "styleguidist server",
    "test": "jest --watchAll",
    "test:ci": "jest --ci",
    "test:coverage": "jest --ci --coverage=true",
    "transpile": "babel src -d dist --extensions '.js,.jsx,.ts,.tsx' --ignore 'src/**/*.test.js*'",
    "posttranspile": "npm run build:types"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test:ci"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.0.2",
    "babel-jest": "^28.1.2",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "docdash": "^1.2.0",
    "eslint": "^8.28.0",
    "eslint-config-es": "^3.25.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-unicorn": "^44.0.2",
    "husky": "^4.2.5",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react": "^18.2.0",
    "react-styleguidist": "^11.2.0",
    "react-test-renderer": "^18.2.0",
    "standard-version": "^9.0.0",
    "style-loader": "^3.3.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.10.14",
    "@mui/material": "^5.8.7"
  }
}
